    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "stat.seqs": "^0.1.21"
  },
  "scripts": {
    "start": "rollup -w -c rollup.umd.config",
    "build": "react-scripts build",
    "build:lib-umd": "NODE_ENV=production rollup -c rollup.umd.config",
    "build:lib-esm": "NODE_ENV=production rollup -c rollup.esm.config",
    "build:lib": "npm run build:lib-umd && npm run build:lib-esm",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
